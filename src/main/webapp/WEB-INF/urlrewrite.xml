<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.2.dtd">
<urlrewrite>
	<rule>
        <from>^/admin$</from>   
        <to>/admin.jsp</to>  
    </rule>
    <rule>
        <from>^/bx/order_timeout$</from>   
        <to>/bx/order_list?fsp.map.if_timeout=0</to>  
    </rule>
    <rule>
        <from>^/bx/([a-zA-Z0-9_]+)</from>   
        <to>/bx!$1.action</to>  
    </rule>
    <rule>
        <from>^/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)$</from>   
        <to>/$1!$2.action</to>  
    </rule>
</urlrewrite>

