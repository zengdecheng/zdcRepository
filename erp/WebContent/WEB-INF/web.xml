<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:jsp="http://java.sun.com/xml/ns/javaee/jsp" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd" id="WebApp_9" version="2.4">
  <filter>
    <filter-name>sitemesh</filter-name>
    <filter-class>com.opensymphony.module.sitemesh.filter.PageFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>sitemesh</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>UrlRewriteFilter</filter-name>
    <filter-class>org.tuckey.web.filters.urlrewrite.UrlRewriteFilter</filter-class>
    <init-param>
      <param-name>logLevel</param-name>
      <param-value>WARN</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>UrlRewriteFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>struts2</filter-name>
    <filter-class>org.apache.struts2.dispatcher.ng.filter.StrutsPrepareAndExecuteFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>struts2</filter-name>
    <url-pattern>*.action</url-pattern>
    <url-pattern>*.jsp</url-pattern>
    <dispatcher>FORWARD</dispatcher>
    <dispatcher>REQUEST</dispatcher>
    <dispatcher>INCLUDE</dispatcher>
  </filter-mapping>
  <jsp-config>
    <taglib>
      <taglib-uri>sitemesh-decorator</taglib-uri>
      <taglib-location>/WEB-INF/tld/sitemesh-decorator.tld</taglib-location>
    </taglib>
    <taglib>
      <taglib-uri>sitemesh-page</taglib-uri>
      <taglib-location>/WEB-INF/tld/sitemesh-page.tld</taglib-location>
    </taglib>
  </jsp-config>
  <servlet>
    <servlet-name>Step3bUpload</servlet-name>
    <servlet-class>com.happyeagle.xbd.servlet.Step3bImgUploadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Step3bUpload</servlet-name>
    <url-pattern>/Step3bUpload</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Step6Upload</servlet-name>
    <servlet-class>com.happyeagle.xbd.servlet.Step6ImgUploadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Step6Upload</servlet-name>
    <url-pattern>/Step6Upload</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DealDownReqUpload</servlet-name>
    <servlet-class>com.happyeagle.xbd.servlet.DealDownReqImgUploadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DealDownReqUpload</servlet-name>
    <url-pattern>/DealDownReqUpload</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FactoryImagUpload</servlet-name>
    <servlet-class>com.happyeagle.xbd.servlet.FactoryImagUploadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FactoryImagUpload</servlet-name>
    <url-pattern>/FactoryImagUpload</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FactoryImagsUpload</servlet-name>
    <servlet-class>com.happyeagle.xbd.servlet.FactoryImagsUploadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FactoryImagsUpload</servlet-name>
    <url-pattern>/FactoryImagsUpload</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>oaOrderImgUpload</servlet-name>
    <servlet-class>com.xbd.oa.servlet.OaOrderImgUploadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>oaOrderImgUpload</servlet-name>
    <url-pattern>/oaOrderImgUpload</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>oaOrderFileUpload</servlet-name>
    <servlet-class>com.xbd.oa.servlet.OaOrderFileUploadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>oaOrderFileUpload</servlet-name>
    <url-pattern>/oaOrderFileUpload</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>oaOrderFileDownload</servlet-name>
    <servlet-class>com.xbd.oa.servlet.OaOrderFileDownloadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>oaOrderFileDownload</servlet-name>
    <url-pattern>/oaOrderFileDownload</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>autoStatistics</servlet-name>
    <servlet-class>com.xbd.oa.servlet.AutoStatisticsServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>autoBx</servlet-name>
    <servlet-class>com.xbd.oa.servlet.AutoBxServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  
  
  
  <error-page>
    <error-code>404</error-code>
    <location>/404.html</location>
  </error-page>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>